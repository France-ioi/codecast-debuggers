{
  "name": "codecast-debuggers",
  "version": "1.0.0",
  "description": "",
  "main": "out/call-script.js",
  "scripts": {
    "prepare": "husky install",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "build:lldb": "docker build -f ./LLDB_Dockerfile . -t lldb-debugger",
    "build:php": "docker build -f ./PHP_Dockerfile . -t php-debugger",
    "build:python": "docker build -f ./Python_Dockerfile . -t python-debugger",
    "build:java": "docker build -f ./Java_Dockerfile . -t java-debugger",
    "debug": "LOG_LEVEL=debug node out/debug-cli.js",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "pretest": "npm run build:ts",
    "server": "LOG_LEVEL=debug node out/server.js",
    "debug_server": "LOG_LEVEL=debug ts-node-dev --respawn src/server.ts",
    "test": "jest src"
  },
  "author": "",
  "license": "ISC",
  "// type": "module",
  "dependencies": {
    "dockerode": "^4.0.0",
    "immer": "^9.0.12",
    "node-debugprotocol-client": "^0.5.0",
    "node-pty": "^0.10.1",
    "recursive-diff": "^1.0.8",
    "tmp": "^0.2.1",
    "ts-command-line-args": "^2.2.1",
    "ts-lsp-client": "^1.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.23",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@types/tmp": "^0.2.3",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.4",
    "vscode-debugprotocol": "^1.50.1"
  }
}
